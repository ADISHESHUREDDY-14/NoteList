<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveNotesServlet</servlet-name>
  	<display-name>SaveNotesServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.SaveNotesServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServlets</servlet-name>
  	<display-name>DeleteServlets</display-name>
  	<description></description>
  	<servlet-class>com.servlets.DeleteServlets</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServlet</servlet-name>
  	<display-name>UpdateServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveNotesServlet</servlet-name>
  	<url-pattern>/SaveNotesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlets</servlet-name>
  	<url-pattern>/DeleteServlets</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServlet</servlet-name>
  	<url-pattern>/UpdateServlet</url-pattern>
  </servlet-mapping>
</web-app>
